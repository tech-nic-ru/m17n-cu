% cases
collating-symbol <MIN>
collating-symbol <CAP>
collating-symbol <SUPER> % Symbolic weight for superscript letter

% variants
collating-symbol <BAS>
collating-symbol <S047B>
collating-symbol <PCL>
collating-symbol <PCL2> % Symbolic weight for second peculiar variant
collating-symbol <PCL3> % Symbolic weight for third peculiar variant
collating-symbol <LIG>

% more letters
collating-symbol <S0479>
collating-symbol <S0463>
collating-symbol <SA657>
collating-symbol <S046B>
collating-symbol <S046F>
collating-symbol <S0471>
collating-symbol <S0473>
collating-symbol <S0475>

% Diacritical marks
collating-symbol <PSILI>
collating-symbol <TITL> % Cyrillic Titlo
collating-symbol <ACA> % 18 accent aigu/acute accent
collating-symbol <GRA> % 20 accent grave/grave accent
collating-symbol <BRE> % 21 brève/breve
collating-symbol <REU> % 25 tréma/diaeresis (ou/or umlaut)
collating-symbol <DAC> % 26 double ac. aigu/double acute ac.
collating-symbol <MAC> % 32 macron/macron
collating-symbol <DGR> % double grave
collating-symbol <IBR> % inverted breve

% additional Church Slavic letters Not implemented in GNU C Library
collating-element <NarrowO> from "/xe1/xb1/xc2"
collating-element <LongDe> from "/xe1/xb1/xc1"
collating-element <WideEs> from "/xe1/xb1/xc3"
collating-element <UKR-IECMB> from "/xea/x99/xb4"
collating-element <CYR-ICMB> from "/xea/x99/xb5"
collating-element <UKR-ICMB>  from "/xea/x99/xb6"
collating-element <CYR-OUCMB>  from "/xea/x99/xb7"
collating-element <SIGDUR-CMB>  from "/xea/x99/xb8"
collating-element <CYRYERU-CMB>  from "/xea/x99/xb9"
collating-element <SIGMOIL-CMB>  from "/xea/x99/xba"
collating-element <CYR-OMEGACMB>  from "/xea/x99/xbb"
collating-element <CYR-EFCMB>  from "/xea/x9a/x9e"

% Contractions
collating-element <o+u> from "<U043E><U0443>"
collating-element <NarrowO+u> from "/xe1/xb1/xc2<U0443>"
collating-element <O+u> from "<U041E><U0443>"
collating-element <O+U> from "<U041E><U0423>"

% Church Slavic Combining Symbols that need to be reordered
collating-element <be-tse-de> from "<U0431><U0446><U2DE3>"
collating-element <BE-tse-de> from "<U0411><U0446><U2DE3>"
